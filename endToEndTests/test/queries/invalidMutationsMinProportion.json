{
  "testCaseName": "query with unvalid minProportions in Mutations action",
  "query": {
    "action": {
      "type": "Mutations",
      "minProportion": -0.5
    },
    "filterExpression": {
      "type": "N-Of",
      "numberOfMatchers": 2,
      "matchExactly": false,
      "children": []
    }
  },
  "expectedQueryResult": {
    "error": "Invalid proportion",
    "message": "minProportion must be in interval (0.0, 1.0]"
  }
}
