{
  "testCaseName": "Details action with order by, limit and offset",
  "query": {
    "action": {
      "type": "Details",
      "orderByFields": ["primary_key"],
      "offset": 9,
      "limit": 2
    },
    "filterExpression": {
      "type": "And",
      "children": [
        {
          "type": "StringEquals",
          "column": "country",
          "value": "Switzerland"
        },
        {
          "type": "Lineage",
          "column": "pango_lineage",
          "value": "B.1.1.7",
          "includeSublineages": true
        }
      ]
    }
  },
