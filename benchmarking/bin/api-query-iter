#!/bin/bash
set -meuo pipefail
IFS=' '

if [ $# -ne 0 ]; then
    echo "usage: all parameters must be passed as env variables"
    exit 1
fi

set -x

"$API_QUERY" iter \
             $API_QUERY_OPTS \
             --repeat "$REPEAT" \
             --concurrency "$CONCURRENCY" \
             "$QUERIES_DIR"/queries.ndjson \
             --drop --log-csv "$API_QUERY_LOG_CSV"

